{
  "extends": "airbnb/base",
  "globals": {
    "jQuery": 1,
    "hljs": 1
  },
  "rules": {
    "func-names": 0,
    "comma-dangle": 0,
    "max-len": 0,
    "consistent-return": 0,
    "arrow-body-style": 0,
    "no-else-return": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0
  }
}
